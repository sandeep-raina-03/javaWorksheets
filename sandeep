    public static void main(String[] args) {
        
        input obj=new abcinput();
        obj.length=10;
        obj.height=12;
        obj.breadth=13;
        double abc=obj.length*obj.height*obj.breadth;
        System.out.println("abc:"+abc);
    }
}
class abcinput
{
    double length;
    double height;
    double breadth;
}
